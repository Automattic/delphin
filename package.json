    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "i18n-calypso": "1.7.0",
    "inherits": "2.0.3",
    "isomorphic-style-loader": "1.0.0",
    "jed": "1.0.2",
    "json-loader": "0.5.4",
    "jstimezonedetect": "1.0.6",
    "lodash": "4.16.4",
    "moment-timezone": "0.5.6",
    "node-sass": "3.10.1",
    "parse-domain": "0.2.2",
    "postcss-loader": "0.8.2",
