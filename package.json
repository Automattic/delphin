{
  "name": "delphin",
  "version": "1.0.0",
  "description": "A registrar client",
  "main": "server/build/bundle.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.config.js --progress --colors",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:static": "BUILD_STATIC=true REFRESH_I18N_CACHE=true node server/build/bundle",
    "clean": "rm -f public/scripts/vendor.*.js && rm -f public/scripts/*.bundle.*.js && rm -f public/scripts/bundle.*.js && rm -f public/styles/bundle.*.css && rm -f public/scripts/assets.json && rm -rf server/build && npm run clean:i18n-cache && npm run clean:static",
    "clean:all": "rm -rf node_modules && npm run clean",
    "clean:i18n-cache": "rm -f server/i18n-cache/data/*",
    "clean:static": "rm -rf public/static",
    "generate": "npm run build && npm run build:static",
    "hooks": "node hooks/manage",
    "lint": "eslint --max-warnings 0 app client server && sass-lint --config .sass-lint.yml --max-warnings 0 --verbose",
    "preinstall": "npm run-script clean",
    "prod:static": "NODE_ENV=production yarn install --ignore-optional && NODE_ENV=production npm run generate",
    "start": "yarn install && npm run build && node server/build/bundle",
    "start:static": "npm run prod:static && node server/static",
    "test": "NODE_PATH=.:app jest --maxWorkers=4",
    "translate": "mkdir -p server/build && i18n-calypso -p delphin -e date -i 'app/**/*.js' -o server/build/delphin.pot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/delphin.git"
  },
  "author": "@automattic",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/Automattic/delphin/issues"
  },
  "homepage": "https://github.com/Automattic/delphin#readme",
  "devDependencies": {
    "async": "^2.0.0-rc.3",
    "babel-eslint": "6.1.2",
    "babel-jest": "^16.0.0",
    "commander": "^2.9.0",
    "eslint": "3.8.1",
    "eslint-config-wpcalypso": "^0.6.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-wpcalypso": "^3.0.2",
    "jest-cli": "16.0.2",
    "minimist": "^1.2.0",
    "npm": "^3.9.6",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "sass-lint": "^1.8.0",
    "serve-static": "^1.10.2"
  },
  "dependencies": {
    "@automattic/dops-components": "github:automattic/dops-components",
    "@automattic/webpack-rtl-plugin": "github:automattic/webpack-rtl-plugin#v2.0.1",
    "async": "^2.0.0-rc.3",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "badwords": "^1.0.0",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.5",
    "cookie-dough": "^0.0.6",
    "creditcards": "2.1.2",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "i18n-calypso": "1.7.0",
    "inherits": "^2.0.1",
    "isomorphic-style-loader": "^1.0.0",
    "jed": "1.0.2",
    "json-loader": "0.5.4",
    "jstimezonedetect": "^1.0.6",
    "lodash": "^4.6.1",
    "moment-timezone": "^0.5.3",
    "node-sass": "^3.10.1",
    "parse-domain": "^0.2.1",
    "postcss-loader": "^0.8.2",
    "pug": "^2.0.0-alpha3",
    "random-words": "0.0.1",
    "react": "15.3.2",
    "react-addons-create-fragment": "^15.0.2",
