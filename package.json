{
  "name": "delphin",
  "version": "0.0.1",
  "description": "A registrar client",
  "main": "server/index.js",
  "scripts": {
    "test": "NODE_PATH=.:app jest",
    "build:client": "webpack --config webpack.config.js --progress --colors",
    "build:server": "webpack --config webpack.server.config.js --verbose --progress --colors",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint --max-warnings 0 app client server",
    "start": "npm install && node build/server.bundle",
    "qs": "npm run build && node build/server.bundle",
    "prod": "NODE_ENV=production node build/server.bundle",
    "postinstall": "npm run-script build",
    "translate": "glotpress-js -p delphin -e date -i build/client.bundle.js -i build/server.bundle.js -o build/delphin.pot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/delphin.git"
  },
  "author": "@automattic",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/Automattic/delphin/issues"
  },
  "homepage": "https://github.com/Automattic/delphin#readme",
  "devDependencies": {
    "async": "^2.0.0-rc.3",
    "babel-eslint": "^6.0.2",
    "commander": "^2.9.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-wpcalypso": "^1.1.3",
    "glotpress-js": "0.0.6",
    "node-sass": "^3.4.2",
    "xgettext-js": "^0.3.0"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-jest": "^11.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.15.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "http-auth": "^2.3.1",
    "inherits": "^2.0.1",
    "jed": "1.0.2",
    "jest-cli": "^11.0.0",
    "json-loader": "^0.5.4",
    "jstimezonedetect": "^1.0.6",
    "lodash": "^4.6.1",
    "lru-cache": "^4.0.1",
    "moment-timezone": "^0.5.3",
    "node-sass": "^3.5.3",
    "pug": "^2.0.0-alpha3",
    "react": "^15.0.1",
    "react-addons-create-fragment": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.3.1",
    "redux-form": "^5.0.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "superagent": "^1.8.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "wpcom": "^4.9.1",
    "wpcom-proxy-request": "^2.0.0",
    "wpcom-unpublished": "^1.1.0"
